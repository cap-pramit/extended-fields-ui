{
  "name": "xaja-integration-poc",
  "version": "0.0.1",
  "description": "Child application that will drop in org-settings-ui application.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Capillary/cap-ui-library"
  },
  "engines": {
    "npm": ">=5 ",
    "node": ">=12.0.0 <15.0.0"
  },
  "author": "Nikhil",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Capillary/cap-ui-library/issues"
  },
  "scripts": {
    "extract-intl": "node ./internals/scripts/extract-intl.js && cp app/translations/en.json dist",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "prebuild": "npm run build:clean",
    "postbuild": "npm run extract-intl && npm run remove-sourcemaps && npm run zip-build",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=production webpack --config ./internals/webpack/webpack.prod.babel.js --color --mode production --progress --stats-optimization-bailout",
    "build:dev": "cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=development webpack --config ./internals/webpack/webpack.prod.babel.js --color --mode production --progress --stats-optimization-bailout",
    "build:analyze": "cross-env NODE_OPTIONS=--max_old_space_size=4096 NODE_ENV=production ANALYZE=true webpack --config ./internals/webpack/webpack.prod.babel.js --color --mode production --progress --stats-optimization-bailout",
    "build:clean": "rimraf ./dist build.zip",
    "start": "cross-env NODE_ENV=development  NODE_OPTIONS=--max_old_space_size=4000 node server",
    "setup": "node ./internals/scripts/app-setup.js",
    "generate": "plop --plopfile ./internals/generators/index.js",
    "remove-sourcemaps": "node ./internals/scripts/remove-sourcemaps.js",
    "zip-build": "node ./internals/scripts/zip-build.js",
    "lint": "npm run lint:js",
    "lint:css": "stylelint './app/**/*.js'",
    "lint:eslint": "eslint --fix",
    "lint:eslint:fix": "eslint --fix",
    "lint:js": "npm run lint:eslint -- app",
    "lint:staged": "lint-staged",
    "prettify": "prettier --write"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "@capillarytech/vulcan-react-sdk": "^0.0.77",
    "node-gyp": "^6.1.0",
